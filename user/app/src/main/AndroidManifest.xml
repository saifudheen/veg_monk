        <activity android:name=".PayPal" />
        <activity android:name=".RazorPay" />
        <activity android:name="com.gofreshuser.GoogleMapWork.Getlatlong" />
        <activity android:name=".ShowLogin" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".MapsActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name="com.gofreshuser.GoogleMapWork.SearchPlaces" />
        <activity android:name=".SplashActtivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.razorpay.ApiKey"
            android:value="rzp_test_OjqPWNjTS6DCNH" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_api_realsekey" />
    </application>

</manifest>